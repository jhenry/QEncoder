# Edit the paths, then rename this file to config.config.

# Path to the daemon queue, used by q_daemon.pl and q_pipe.pl
daemonpath=/path/to/something/like/var/local/spool/qdaemon

# Paths used by the shell script that is run by systemd unit
Q_DAEMON_PATH="/path/to/cumulusclips/cc-content/plugins/QEncoder/bin/q_daemon.pl"
LOG_PATH="/path/to/something/like/var/log/cumulusclips/q.log"
